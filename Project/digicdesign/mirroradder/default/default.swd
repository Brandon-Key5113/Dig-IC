analog2digital -sourcewf <mirroradder_default/TRAN>V(B) -threshold 0.54 Dig_V(B)
analog2digital -sourcewf <mirroradder_default/TRAN>V(CIN) -threshold 0.54 Dig_V(CIN)
analog2digital -sourcewf <mirroradder_default/TRAN>V(COUT) -threshold 0.54032 Dig_V(COUT)
